<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<PackageId>FluentOpenApi</PackageId>
		<Version>1.0.0</Version>
		<Authors>yataofeng</Authors>
		<Description>A fluent framework for defining OpenAPI schemas and validation in .NET</Description>
		<RepositoryUrl>https://github.com/yataofeng/FluentOpenApi</RepositoryUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageTags>openapi, fluent, validation, aspnetcore</PackageTags>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.OpenApi" Version="9.0.2" />
	</ItemGroup>
	<ItemGroup>
		<FrameworkReference Include="Microsoft.AspNetCore.App" />
	</ItemGroup>
</Project>
  